<?xml version="1.0"?>
<package type="addon" name="nuxeo-coldstorage" version="@VERSION@">
  <title>Nuxeo Cold Storage</title>
  <description>The Nuxeo Cold Storage addon allows the storage of the document main content in a cold storage. This can
    be needed for archiving, compliance, etc.
  </description>
  <home-page>https://doc.nuxeo.com/coldstorage/</home-page>
  <vendor>Nuxeo</vendor>
  <target-platform>
    <name>@DISTRIBUTION_NAME@</name>
    <version>[@NUXEO_PLATFORM_VERSION@,2023.0)</version>
  </target-platform>
  <!-- Dependencies on other packages -->
  <dependencies>
    <package>amazon-s3-online-storage</package>
    <package>nuxeo-web-ui</package>
  </dependencies>
  <license>Apache 2.0</license>
  <license-url>http://www.apache.org/licenses/LICENSE-2.0</license-url>
</package>
